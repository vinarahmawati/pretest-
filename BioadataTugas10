public class BiodataTugas10{
	String nama;
	int umur;
	String alamat;

	void setNama(String n){
		nama = n;
	}

	void setUmur(int u){
		umur = u;
	}

	void setAlamat(String a){
		alamat = a;
	}

	String getNama(){
		return nama;
	}

	int getUmur(){
		return umur;
	}

	String getAlamat(){
		return alamat;
	}
}
